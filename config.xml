<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.hello" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>remy</name>
    <icon density="mdpi" height="57" platform="android" src=".\www\Arrow Color Bg@5x.png" width="57" />
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-googleplus" spec="~7.0.0">
        <variable name="REVERSED_CLIENT_ID" value="com.googleusercontent.apps.921814792691-reh6oij417djlfh12pv311a0t0e5pjin" />
        <variable name="WEB_APPLICATION_CLIENT_ID" value="921814792691-reh6oij417djlfh12pv311a0t0e5pjin.apps.googleusercontent.com" />
    </plugin>
    <engine name="android" spec="^7.1.4" />
</widget>
